{
  "name": "@instamint/business-server",
  "version": "24.05",
  "description": "Business API service for Instamint",
  "type": "commonjs",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "tsc --project tsconfig.json",
    "dev": "tsx watch ./src/index.ts",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "test": "cross-env NODE_ENV=test jest --forceExit --verbose --maxWorkers=1",
    "test:ci": "cross-env NODE_ENV=test jest --forceExit --maxWorkers=3 --ci",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "kn:latest": "knex migrate:latest",
    "kn:rollback": "knex migrate:rollback",
    "kn:make": "knex migrate:make $1",
    "kn:seed": "knex seed:run"
  },
  "dependencies": {
    "@cloudflare/workers-types": "4.20240423.0",
    "@hono/node-server": "^1.11.0",
    "@hono/sentry": "^1.0.1",
    "@hono/zod-validator": "^0.2.1",
    "@instamint/shared-types": "workspace:^",
    "@jest/globals": "^29.7.0",
    "@sendgrid/mail": "^8.1.1",
    "dotenv": "^16.4.5",
    "hono": "^4.2.7",
    "ioredis": "^5.3.2",
    "jest": "^29.7.0",
    "knex": "^3.1.0",
    "objection": "^3.1.4",
    "otplib": "^12.0.1",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "rate-limiter-flexible": "^4.0.1",
    "socket.io": "^4.7.5",
    "ts-node": "^10.9.2",
    "undici": "^6.7.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@instamint/eslint-config-server": "workspace:^",
    "@instamint/server-types": "workspace:^",
    "@instamint/tsconfig-server": "workspace:^",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@types/qrcode": "^1.5.5",
    "@types/socket.io": "^3.0.2",
    "cross-env": "^7.0.3",
    "esbuild-jest": "^0.5.0",
    "tsx": "^4.15.7",
    "typescript": "^5.4.2"
  },
  "license": "All Rights Reserved"
}
